sp {apply*compute-challenge-pass-likelihood*impossible*pass*prob-only
	#:interrupt
   (state <s> ^operator <op>
              ^superstate.operator.evaluation <e>)
   (<op> ^name compute-challenge-pass-likelihood
         ^target.pushed <push1>)
   (<push1> ^die.face <n>
            ^die.face <> <n>)
-->
   (write (crlf) |  Challenge success likelihood is 1.0.|)
   (<e> ^probability 1.0)
}

