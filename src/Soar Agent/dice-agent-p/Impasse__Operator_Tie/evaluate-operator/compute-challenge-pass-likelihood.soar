sp {apply*compute-challenge-pass-likelihood*reasonable*pass*1-under
   (state <s> ^operator <op>
              ^superstate.operator.evaluation <e>)
   (<op> ^name compute-challenge-pass-likelihood
         ^target <target>)
   (<target> ^cup <cup>)
   (<cup> ^count 1)
  -{(<target> ^pushed <push1>)
    (<push1> ^die.face <n>
             ^die.face <> <n>)}
-->
   (write (crlf) |  Challenge success likelihood is .2.|)
   (<e> ^probability .20)}

sp {apply*compute-challenge-pass-likelihood*unlikely*pass*not-all-under*more-two
   (state <s> ^operator <op>
              ^superstate.operator.evaluation <e>)
   (<op> ^name compute-challenge-pass-likelihood
         ^target <target>)
   (<target> ^cup.count 2
             ^pushed.count > 0)
 -{(<target> ^pushed <push1>)  # Add 5/15/2014 so it doesn't override certain challenge.
   (<push1> ^die.face <n>
            ^die.face <> <n>)}
-->
   (write (crlf) |  Challenge success likelihood is safe.|)
   (<e> ^probability .95)
}

