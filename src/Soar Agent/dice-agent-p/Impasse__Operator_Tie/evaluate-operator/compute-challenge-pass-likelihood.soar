sp {apply*compute-challenge-pass-likelihood*unlikely*pass*all-under*three
   (state <s> ^operator <op>
              ^superstate.operator.evaluation <e>)
   (<op> ^name compute-challenge-pass-likelihood
         ^target <target>)
   (<target> ^cup.count 3
             ^pushed.count 0)
-->
   (write (crlf) |  Challenge pass success likelihood is .9.|)
   (<e> ^probability .95)}

sp {apply*compute-challenge-pass-likelihood*unlikely*pass*all-under*more-two
   (state <s> ^operator <op>
              ^superstate.operator.evaluation <e>)
   (<op> ^name compute-challenge-pass-likelihood
         ^target <target>)
   (<target> ^cup.count > 3
             ^pushed.count 0)
-->
   (write (crlf) |  Challenge pass success likelihood is .9.|)
   (<e> ^probability 1.0)}

