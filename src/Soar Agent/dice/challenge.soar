sp {dice*propose*challenge
   (state <s> ^name dice
              ^my-turn true
              ^io.input-link.affordances.action <a>)
   (<a> ^name challenge
        ^available true
        ^target <t>)
-->
   (<s> ^operator <op> + =)
   (<op> ^name challenge
         ^target <t>)
}

sp {apply*challenge
   (state <s> ^operator <op>
              ^io.output-link <ol>)
   (<op> ^name challenge
         ^target.id <target>)
-->
   (<ol> ^challenge.target <target>)
}